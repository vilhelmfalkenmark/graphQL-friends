query PostsForAuthor {
author(id: 1) {
  firstName
  posts {
    title
    votes
  }
}
}

query PostsForAuthor2 {
  posts {
    id,
    title,
    votes
  }
}

mutation PostsForAuthor3 {
  upvotePost(postId:2) {
    id,
    votes
  }
}


